</section><script>
        function getElementBySelector(selector)
{
    element = document.querySelectorAll(selector).item(0);
    return element;
}


        var toogleButton = getElementBySelector('#js_566a968a268e3ece348b4585 .js-toggle-widget .js-botonera-toggle');
        var inicialElement = getElementBySelector('#js_566a968a268e3ece348b4585 .js-toggle-widget');
        
        if (toogleButton != null && inicialElement != null) {
            var offsetHeight = inicialElement.scrollHeight;
            var maxHiddenHeight = 0; 
            var counter = 0;
            while (inicialElement.children.item(counter)) {
                if (inicialElement.children.item(counter).clientHeight == 0) {
                    maxHiddenHeight = maxHiddenHeight + inicialElement.children.item(counter).scrollHeight;
                    inicialElement.children.item(counter).style.height = inicialElement.children.item(counter).scrollHeight + "px";
                }
                counter++;
            }
        
            inicialElement.style.height = offsetHeight + "px";
            var maxHeight = maxHiddenHeight + offsetHeight;

            var toogleActive = false;
var toogleState = "closed";
var timeoutHandle = null;

function slowToogle()
{
        var pixelsJump = 3;
    
    if (toogleState == "opening") {
        var height;
        if ((maxHeight - inicialElement.clientHeight) > pixelsJump) {
            height = inicialElement.clientHeight + pixelsJump;
        } else {
            height = maxHeight;
        }
        inicialElement.style.height = height + 'px';
        if (inicialElement.clientHeight >= maxHeight) {
            clearInterval(timeoutHandle);
            toogleState = "opened";
        }
    } else if (toogleState == "closing") {
        var height;
        if (inicialElement.clientHeight > pixelsJump) {
            height = inicialElement.clientHeight - pixelsJump;
        } else {
            height = offsetHeight;
        }
        inicialElement.style.height = height + 'px';
        if (inicialElement.clientHeight <= offsetHeight) {
            clearInterval(timeoutHandle);
            toogleState = "closed";
        }
    }
}

function toogleElement()
{
    if (timeoutHandle != null) {
        clearInterval(timeoutHandle);
        timeoutHandle = null;
    }

    if (toogleState == "closed" || toogleState == "closing") {
        toogleState = "opening";
    } else {
        toogleState = "closing";
    }

    timeoutHandle = setInterval(slowToogle, 15);
}


            toogleButton.onclick = function(){
                toogleElement();
            };
        }
    </script><aside class="content-ad"><div class="ad-item-990"><div id='div-gpt-ad-portada_c990x50_320x53c_566a968a268e3ece348b4585' class="ad-item"><script type='text/javascript'>googletag.cmd.push(function(){googletag.display('div-gpt-ad-portada_c990x50_320x53c_566a968a268e3ece348b4585');});</script></div></div></aside><div id="portada" class="contenido"><section class="row-layout"><div class="container"><ul class="content-items flex-a"><li class="content-item "><article class=" mod-item  mod-futbol
